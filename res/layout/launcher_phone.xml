<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:layout_width="match_parent"
	android:layout_height="match_parent">
	<Button android:id="@+id/launcher_login_btn_phone"
		android:layout_height="wrap_content" android:layout_width="150dp"
		android:layout_alignParentTop="true" android:layout_centerHorizontal="true"
		android:layout_marginTop="128dp" android:text="@string/launcher_login_btn_text">
	</Button>
	<Button android:id="@+id/launcher_register_btn_phone"
		android:layout_height="wrap_content" android:layout_width="150dp"
		android:layout_below="@+id/launcher_login_btn_phone"
		android:layout_alignLeft="@+id/launcher_login_btn_phone"
		android:layout_marginTop="60dp" android:text="@string/launcher_register_btn_text"></Button>

</RelativeLayout>
